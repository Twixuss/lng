import "console.lng"
import "list.lng"
printList function(list List) {
    console.write("List:\nCount: ")
    console.write(list.count())
    console.write("\nCapacity: ")
    console.writeLine(list.capacity())
    while list.begin < list.end {
        console.writeLine(^list.begin)
        list.begin += 1
    }
}
